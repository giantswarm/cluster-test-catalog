{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "baseDomain": {
            "type": "string"
        },
        "bastion": {
            "type": "object",
            "properties": {
                "allowlistSubnets": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "instanceType": {
                    "type": "string"
                },
                "replicas": {
                    "type": "integer"
                }
            }
        },
        "clusterDescription": {
            "type": "string"
        },
        "clusterName": {
            "type": "string"
        },
        "controlPlane": {
            "type": "object",
            "properties": {
                "apiAllowlistSubnets": {
                    "type": "string"
                },
                "containerdVolume": {
                    "type": "object",
                    "properties": {
                        "sizeGB": {
                            "type": "integer"
                        },
                        "diskType": {
                            "type": "string"
                        }
                    }
                },
                "customNodeTaints": {
                    "type": "array"
                },
                "etcd": {
                    "type": "object",
                    "properties": {
                        "imageRepository": {
                            "type": "string"
                        },
                        "imageTag": {
                            "type": "string"
                        }
                    }
                },
                "etcdVolume": {
                    "type": "object",
                    "properties": {
                        "sizeGB": {
                            "type": "integer"
                        },
                        "diskType": {
                            "type": "string"
                        }
                    }
                },
                "instanceType": {
                    "type": "string"
                },
                "kubeletVolume": {
                    "type": "object",
                    "properties": {
                        "sizeGB": {
                            "type": "integer"
                        },
                        "diskType": {
                            "type": "string"
                        }
                    }
                },
                "rootVolume": {
                    "type": "object",
                    "properties": {
                        "sizeGB": {
                            "type": "integer"
                        },
                        "diskType": {
                            "type": "string"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "email": {
                            "type": "string"
                        },
                        "scopes": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "gcp": {
            "type": "object",
            "properties": {
                "failureDomains": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "project": {
                    "type": "string"
                },
                "region": {
                    "type": "string"
                }
            }
        },
        "hashSalt": {
            "type": "string"
        },
        "includeClusterResourceSet": {
            "type": "boolean"
        },
        "kubectlImage": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "kubernetesVersion": {
            "type": "string"
        },
        "machineDeployments": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "containerdVolume": {
                        "type": "object",
                        "properties": {
                            "sizeGB": {
                                "type": "integer"
                            },
                            "diskType": {
                                "type": "string"
                            }
                        }
                    },
                    "customNodeLabels": {
                        "type": "array"
                    },
                    "customNodeTaints": {
                        "type": "array"
                    },
                    "failureDomain": {
                        "type": "string"
                    },
                    "instanceType": {
                        "type": "string"
                    },
                    "kubeletVolume": {
                        "type": "object",
                        "properties": {
                            "sizeGB": {
                                "type": "integer"
                            },
                            "diskType": {
                                "type": "string"
                            }
                        }
                    },
                    "name": {
                        "type": "string"
                    },
                    "replicas": {
                        "type": "integer"
                    },
                    "rootVolume": {
                        "type": "object",
                        "properties": {
                            "sizeGB": {
                                "type": "integer"
                            },
                            "diskType": {
                                "type": "string"
                            }
                        }
                    },
                    "serviceAccount": {
                        "type": "object",
                        "properties": {
                            "email": {
                                "type": "string"
                            },
                            "scopes": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "network": {
            "type": "object",
            "properties": {
                "nodeSubnetCidr": {
                    "type": "string"
                },
                "podCidr": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "proxySubnetCidr": {
                    "type": "string"
                },
                "serviceCIDR": {
                    "type": "string"
                }
            }
        },
        "oidc": {
            "type": "object",
            "properties": {
                "caFile": {
                    "type": "string"
                },
                "clientId": {
                    "type": "string"
                },
                "groupsClaim": {
                    "type": "string"
                },
                "issuerUrl": {
                    "type": "string"
                },
                "usernameClaim": {
                    "type": "string"
                }
            }
        },
        "organization": {
            "type": "string"
        },
        "sshSSOPublicKey": {
            "type": "string"
        },
        "ubuntuImageVersion": {
            "type": "string"
        },
        "vmImageBase": {
            "type": "string"
        }
    }
}
